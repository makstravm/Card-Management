{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@babel-eslint",
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },

  "rules": {
    "no-console": 1,
    "no-param-reassign": 0,
    "quotes": "off",
    "linebreak-style": ["off", "windows"]
  }
}
